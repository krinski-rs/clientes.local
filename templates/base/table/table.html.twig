            						<div class="table-responsive">
            							<table class="table table-striped table-bordered table-hover" id="{{ content.table.id }}"  style="width:100%">
{% if attribute(content.table, 'header') is defined %}
            								<thead>
            									<tr>
{% for key, header in content.table.header %}
            										<th>{{ header }}</th>
{% endfor %}
            									</tr>
            								</thead>
{% endif %}
            								<tbody>
{% if attribute(content.table, 'body') is defined %}
{% for key, body in content.table.body %}
            									<tr {% if ((key|round +1) % 2) == 0 %}  class="gradeA even" {% else %} class="gradeA odd" {% endif %}	>
{% for key, t in body %}

{% if t is iterable %}
            										<td>
{% for key, b in t %}
        {% if key|round > 0 %}
        <br/>
        {% endif %}
        {{ b|join(' ') }}
 {% endfor %}
            										</td>
{% else %}
            										<td>{{ t }}</td>
{% endif %}
{% endfor %}
            									</tr>
{% endfor %}
{% endif %}
